{
  "expo": {
    "name": "tiana-bakeshop",
    "slug": "tiana-bakeshop",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ernestechie.tianabakeshop",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-android.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ernestechie.tianabakeshop"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            {
              "fontFamily": "OpenSans",
              "fontDefinitions": [
                {
                  "path": "./assets/fonts/OpenSans-Bold.ttf",
                  "weight": 600
                },
                {
                  "path": "./assets/fonts/OpenSans-Regular.ttf",
                  "weight": 400
                }
              ]
            }
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/splash-icon.png",
          "dark": {
            "image": "./assets/splash-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "66f5de8b-19d2-4f34-be62-cc74976f2f5f"
      }
    }
  }
}
